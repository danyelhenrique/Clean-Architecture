{
	"env": {
		"es2020": true,
		"node": true
	},
	"extends": ["standard", "plugin:prettier/recommended"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 11,
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
		"no-tabs": 0,
		"prettier/prettier": ["error"],
		"indent": 0
	}
}
